{  "info": {    "name": "CollabHub Phase 2",    "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"  },  "item": [    {      "name": "Tasks",      "item": [        {          "name": "Create Task",          "request": {            "method": "POST",            "header": [              {                "key": "Authorization",                "value": "Bearer {{TOKEN}}"              }            ],            "url": "{{BASE_URL}}/api/tasks",            "body": {              "mode": "raw",              "raw": {                "name": "Implement Login Feature",                "description": "Create login page with authentication",                "team_id": "your_team_id",                "estimated_hours": 8,                "start_date": "2024-03-20",                "due_date": "2024-03-27",                "priority": 1              },              "options": {                "raw": {                  "language": "json"                }              }            }          }        },        {          "name": "Get Team Tasks",          "request": {            "method": "GET",            "header": [              {                "key": "Authorization",                "value": "Bearer {{TOKEN}}"              }            ],            "url": "{{BASE_URL}}/api/tasks/team/{{TEAM_ID}}"          }        },        {          "name": "Update Task Progress",          "request": {            "method": "PUT",            "header": [              {                "key": "Authorization",                "value": "Bearer {{TOKEN}}"              }            ],            "url": "{{BASE_URL}}/api/tasks/{{TASK_ID}}/progress",            "body": {              "mode": "raw",              "raw": {                "actual_hours": 4,                "status": "in_progress"              },              "options": {                "raw": {                  "language": "json"                }              }            }          }        }      ]    },    {      "name": "Task Comments",      "item": [        {          "name": "Add Comment",          "request": {            "method": "POST",            "header": [              {                "key": "Authorization",                "value": "Bearer {{TOKEN}}"              }            ],            "url": "{{BASE_URL}}/api/tasks/{{TASK_ID}}/comments",            "body": {              "mode": "raw",              "raw": {                "comment": "This is a test comment"              },              "options": {                "raw": {                  "language": "json"                }              }            }          }        },        {          "name": "Get Task Comments",          "request": {            "method": "GET",            "header": [              {                "key": "Authorization",                "value": "Bearer {{TOKEN}}"              }            ],            "url": "{{BASE_URL}}/api/tasks/{{TASK_ID}}/comments"          }        }      ]    },    {      "name": "Meetings",      "item": [        {          "name": "Create Meeting",          "request": {            "method": "POST",            "header": [              {                "key": "Authorization",                "value": "Bearer {{TOKEN}}"              }            ],            "url": "{{BASE_URL}}/api/meetings",            "body": {              "mode": "raw",              "raw": {                "title": "Sprint Planning",                "agenda": "Plan next sprint tasks",                "meeting_time": "2024-03-21T10:00:00Z",                "team_id": "{{TEAM_ID}}",                "duration_minutes": 60              },              "options": {                "raw": {                  "language": "json"                }              }            }          }        },        {          "name": "Invite Attendees",          "request": {            "method": "POST",            "header": [              {                "key": "Authorization",                "value": "Bearer {{TOKEN}}"              }            ],            "url": "{{BASE_URL}}/api/meetings/{{MEETING_ID}}/attendees",            "body": {              "mode": "raw",              "raw": {                "attendees": ["user_id_1", "user_id_2"]              },              "options": {                "raw": {                  "language": "json"                }              }            }          }        },        {          "name": "Generate Meeting URL",          "request": {            "method": "POST",            "header": [              {                "key": "Authorization",                "value": "Bearer {{TOKEN}}"              }            ],            "url": "{{BASE_URL}}/api/meetings/{{MEETING_ID}}/generate-url"          }        }      ]    },    {      "name": "Task Dependencies",      "item": [        {          "name": "Add Dependency",          "request": {            "method": "POST",            "header": [              {                "key": "Authorization",                "value": "Bearer {{TOKEN}}"              }            ],            "url": "{{BASE_URL}}/api/tasks/{{TASK_ID}}/dependencies",            "body": {              "mode": "raw",              "raw": {                "dependent_task_id": "another_task_id"              },              "options": {                "raw": {                  "language": "json"                }              }            }          }        },        {          "name": "Get Dependencies",          "request": {            "method": "GET",            "header": [              {                "key": "Authorization",                "value": "Bearer {{TOKEN}}"              }            ],            "url": "{{BASE_URL}}/api/tasks/{{TASK_ID}}/dependencies"          }        }      ]    }  ]}